{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "moduleResolution": "Node",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "types": ["vite/client"]
  },
  "include": ["./src", "thumbnail_test.tsx"]
}
